{
    "version": 2,
    "builds": [
      {
        "src": "shopsmart-app/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "shopsmart-app/build" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "shopsmart-app/build/index.html"
      }
    ]
  }
  